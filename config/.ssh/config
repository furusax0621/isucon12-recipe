Host github-repository-recipe
  User git
  Port 22
  HostName github.com
  IdentityFile ~/.ssh/isucon_recipe_ed25519
  TCPKeepAlive yes
  IdentitiesOnly yes
Host github.com
  User git
  Port 22
  HostName github.com
  IdentityFile ~/.ssh/isucon_code_ed25519
  TCPKeepAlive yes
  IdentitiesOnly yes

